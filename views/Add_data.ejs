<form action="/addStudentDetails" method="post" enctype="multipart/form-data">
    <h2>Student details</h2>
    <table border="1">
        <tr>
            <td>Name :</td>
            <td><input type="text" name="name"></td><br>
        </tr>
        <tr>
            <td>Age :</td>
            <td><input type="text" name="age"></td><br>
        </tr>
        <tr>
            <td>Gender :</td>
            <td>
                <input type="radio" name="gender" value="male">Male
                <input type="radio" name="gender" value="female">Female
            </td>
        </tr>
        <tr>
            <td>Hobby :</td>
            <td>
                <input type="checkbox" name="hobby" value="reading">Reading
                <input type="checkbox" name="hobby" value="cycling">Cycling
                <input type="checkbox" name="hobby" value="eating">Eating
            </td>
        </tr>
        <tr>
            <td>City :</td>
            <% var city=["Surat","Rajkot","Vadodara","Godhara"] %>
                <td>
                    <select name="city">
                        <option value="">-- Select City -- </option>
                        <% city.map((v)=> { %>
                            <option value="<%= v %>">
                                <%= v %>
                            </option>
                            <% }) %>
                    </select>
                </td>
        </tr>
        <tr>
            <td>Message :</td>
            <td>
                <textarea name="message" cols="30" rows="5"></textarea>
            </td>
        </tr>
        <tr>
            <td colspan="2"><input type="file" name="adminImage"></td>
        </tr>
        <tr>
            <td><input type="submit" name="submit" value="Add Student"></td>
        </tr>
    </table>
</form>
<a href="/View_data">View Data</a>