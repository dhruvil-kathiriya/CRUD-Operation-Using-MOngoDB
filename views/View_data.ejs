<a href="/">ADD Data</a>

<table border="1">
    <h2>Student Details</h2>
    <tr>
        <td>Name</td>
        <td>Age</td>
        <td>Gender</td>
        <td>Hobby</td>
        <td>City</td>
        <td>Message</td>
        <td>Image</td>
        <td>Action</td>
    </tr>
    <% for (var st of stData){ %>
        <tr>
            <td>
                <%= st.name %>
            </td>
            <td>
                <%= st.age %>
            </td>
            <td>
                <%= st.gender %>
            </td>
            <td>
                <%= st.hobby %>
            </td>
            <td>
                <%= st.city %>
            </td>
            <td>
                <%= st.message %>
            </td>
            <td>
                <img src="<%= st.adminImage %>" height="100">
            </td>
            <td>
                <a href="/delteStudentDetails/<%= st.id %>">Delete</a> ||
                <a href="/updateStudentDetails/<%= st.id %>">Update</a>
            </td>
        </tr>
        <% }%>
</table>